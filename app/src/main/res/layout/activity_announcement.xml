<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/ruqjnp76ywjd"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#F1F5F9">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rrn30ks8pvun"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="#FFD133"
                android:paddingVertical="20dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="24dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ic_back"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/ic_backarrow"
                    android:layout_marginEnd="8dp"
                    android:orientation="vertical">
                </ImageView>
                <TextView
                    android:id="@+id/tv_title_announcement"
                    android:textColor="#0F172A"
                    android:textSize="20sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Pengumuman"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r6px1suksp3f"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_announcement"
                android:paddingVertical="26dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="14dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ru9y7spc3a3b"
                    android:textColor="#252525"
                    android:textSize="14sp"
                    android:layout_marginBottom="13dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Pre-Test Solution Culture"
                    />
                <TextView
                    android:id="@+id/r8lkjnuwa51e"
                    android:textColor="#C62828"
                    android:textSize="10sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Dateline Selasa, 24 September 2024"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r5c1jh5lv7e"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_announcement"
                android:paddingVertical="26dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="14dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/rojgbpm1o1b"
                    android:textColor="#252525"
                    android:textSize="14sp"
                    android:layout_marginBottom="13dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Pre-Test Innovation Management"
                    />
                <TextView
                    android:id="@+id/r69149j9hnm"
                    android:textColor="#C62828"
                    android:textSize="10sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Dateline Selasa, 1 Oktober 2024"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/r0q2b7dk98e2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bg_announcement"
                android:paddingVertical="26dp"
                android:paddingHorizontal="20dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r3y5y8yag4et"
                    android:textColor="#252525"
                    android:textSize="14sp"
                    android:layout_marginBottom="13dp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Pre-Test Business Process"
                    />
                <TextView
                    android:id="@+id/rz0hc237kqr"
                    android:textColor="#C62828"
                    android:textSize="10sp"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Dateline Jumat, 4 Oktober 2024"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>