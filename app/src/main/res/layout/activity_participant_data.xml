<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/re114909vy85"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:scrollbars="none"
        android:background="#F1F5F9">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/rdrjbvtcpsdj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="#FFD133"
                android:paddingVertical="20dp"
                android:paddingHorizontal="20dp"
                android:layout_marginBottom="30dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/ic_back"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginEnd="20dp"
                    android:src="@drawable/ic_backarrow"
                    android:scaleType="fitXY" />

                <TextView
                    android:id="@+id/ralbyt6fx9bd"
                    android:textColor="#0F172A"
                    android:textSize="20sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="Participant Data" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rxk0ihu81brm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/bg_card2"
                android:backgroundTint="@color/shades_50"
                android:paddingVertical="15dp"
                android:paddingHorizontal="16dp"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginEnd="12dp"
                    android:src="@mipmap/ic_launcher"
                    app:civ_border_color="#FF000000" />

                <LinearLayout
                    android:id="@+id/rk4w5ai6214m"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_nama"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Nama           : "
                            />
                        <TextView
                            android:id="@+id/nama"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Muhammad Richo "
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_posisi"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Posisi           : "
                            />
                        <TextView
                            android:id="@+id/posisi"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="UI/UX Designer"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_universitas"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Universitas  : "
                            />
                        <TextView
                            android:id="@+id/universitas"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Universitas Brawijaya"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_jurusan"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Jurusan       : "
                            />
                        <TextView
                            android:id="@+id/jurusan"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Teknik Informatika "
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/card2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/bg_card2"
                android:backgroundTint="@color/shades_50"
                android:paddingVertical="15dp"
                android:paddingHorizontal="16dp"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="horizontal">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profile_image2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:layout_marginEnd="12dp"
                    android:src="@mipmap/ic_launcher"
                    app:civ_border_color="#FF000000" />

                <LinearLayout
                    android:id="@+id/content"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_nama2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Nama           : "
                            />
                        <TextView
                            android:id="@+id/nama2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Della Fitria Lestari"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_posisi2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Posisi           : "
                            />
                        <TextView
                            android:id="@+id/posisi2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Programmer"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_universitas2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Universitas  : "
                            />
                        <TextView
                            android:id="@+id/universitas2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Universitas Muhammadiyah SUrakarta"
                            />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:layout_marginBottom="6dp"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/title_jurusan2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_marginEnd="9dp"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:text="Jurusan       : "
                            />
                        <TextView
                            android:id="@+id/jurusan2"
                            android:textColor="#000000"
                            android:textSize="12sp"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:text="Teknik Informatika "
                            />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>
